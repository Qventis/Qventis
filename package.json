{
  "name": "qventis-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lighthouse --preset=desktop --output-path=./lighthouse-desktop.html --output=html http://localhost:3000",
    "lighthouse:mobile": "lighthouse --preset=perf --output-path=./lighthouse-mobile.html --output=html http://localhost:3000",
    "performance:audit": "npm run build && npm run start & sleep 5 && npm run lighthouse && kill %1",
    "seo:audit": "lighthouse --only-categories=seo --output-path=./seo-audit.html --output=html http://localhost:3000",
    "analyze": "ANALYZE=true npm run build",
    "speed:test": "node scripts/speed-test.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@vercel/analytics": "^1.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "next": "15.3.5",
    "next-seo": "^6.8.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.59.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.73"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "@next/bundle-analyzer": "^15.1.0",
    "@lhci/cli": "^0.12.0",
    "lighthouse": "^11.0.0",
    "puppeteer": "^21.0.0",
    "web-vitals": "^3.5.0"
  }
}
